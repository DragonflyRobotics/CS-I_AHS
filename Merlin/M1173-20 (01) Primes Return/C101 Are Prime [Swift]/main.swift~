// Add your code below:
import Foundation

func isPrime(n: Int) -> Bool {
    if n < 2 {
        return false
    }
    for i in 2...Int(Math.sqrt(Double(n)))+1 {
        if (n%i) == 0 {
            return false
        }
    }
    return true
}






// Add your code above

// ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩  ⇩
// DO NOT ALTER THE TEXT BETWEEN THESE LINES =========={M{E{R{L{I{N{1}N}I}L}R}E}M}=====================================
let n = 7
print(n, isPrime(n:n))
