let number : Int = 5
// DO NOT ALTER THE TEXT BETWEEN THESE LINES =========={M{E{R{L{I{N{1}N}I}L}R}E}M}=====================================
// ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧


// Add your code below:

func num2Word(num: Int) -> String {
    switch num {
    case 0:
        return "zero"
    case 1:
        return "one"
    case 2:
        return "two"
    case 3:
        return "three"
    case 4:
        return "four"
    case 5:
        return "five"
    case 6:
        return "six"
    case 7:
        return "seven"
    case 8:
        return "eight"
    case 9:
        return "nine"
    default:
        return "None"
    }
    //return "None"
}

print(num2Word(num: number))
